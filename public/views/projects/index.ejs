<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Projects</title>
</head>
<body>
  <h1>Projects</h1>
  <a href="/projects/new">Create New Project</a>
  <ul>
    <% projects.forEach(project => { %>
      <li>
        <a href="/projects/<%= project._id %>"><%= project.name %></a>
      </li>
    <% }) %>
  </ul>
</body>
</html>
